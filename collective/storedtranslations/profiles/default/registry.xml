<?xml version="1.0"?>
<registry xmlns:i18n="http://xml.zope.org/namespaces/i18n"
          i18n:domain="plone">
  <records interface="collective.storedtranslations.interfaces.IStoredTranslationsSettings" />
  <record name="collective.storedtranslations.translationdomains">
    <field type="plone.registry.field.Dict">
      <title i18n:translate="">Translation domains</title>
      <key_type type="plone.registry.field.TextLine">
        <title i18n:translate="">Domain</title>
      </key_type>
      <value_type type="plone.registry.field.Dict">
        <title i18n:translate="">Language</title>
        <key_type type="plone.registry.field.TextLine">
          <title i18n:translate="">Language Code</title>
        </key_type>
        <value_type type="plone.registry.field.Dict">
          <title i18n:translate="">Message</title>
          <required>False</required>
          <key_type type="plone.registry.field.TextLine">
            <title i18n:translate="">Message ID</title>
          </key_type>
          <value_type type="plone.registry.field.TextLine">
            <title i18n:translate="">Message String</title>
            <required>False</required>
          </value_type>
        </value_type>
      </value_type>
    </field>
    <value purge="false" />
  </record>
</registry>
