<html xmlns="http://www.w3.org/1999/xhtml" xml:lang="en"
      xmlns:tal="http://xml.zope.org/namespaces/tal"
      xmlns:metal="http://xml.zope.org/namespaces/metal"
      xmlns:i18n="http://xml.zope.org/namespaces/i18n"
      lang="en"
      metal:use-macro="here/prefs_main_template/macros/master"
      i18n:domain="collective.storedtranslations">

<body>

<div metal:fill-slot="prefs_configlet_main">

    <a href=""
       id="setup-link"
       tal:attributes="href string:$portal_url/@@overview-controlpanel"
       i18n:domain="plone"
       i18n:translate="">
        Site Setup
    </a>

    <h1 class="documentFirstHeading" tal:content="view/label">View Title</h1>

    <div metal:use-macro="context/global_statusmessage/macros/portal_message">
      Portal status message
    </div>
    

    
    <div id="content-core">
      <a tal:attributes="href string:${portal_url}/portal_registry/edit/collective.storedtranslations.translationdomains"
         i18n:translate="">Update the stored translations</a>

        <span tal:content="view/form/domain|nothing" />
        <span tal:content="view/form/language|nothing" />

        <div id="layout-contents">
            <span tal:replace="structure view/contents" />
        </div>
    </div>
    
</div>
</body>
</html>
